<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<title>
		<%= title %>
	</title>

	<!-- Angular Material style sheet -->
	<link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/angular_material/<%= v_angularMaterial %>/angular-material.min.css">
	<!-- Angular JS -->
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularjs %>/angular.min.js" type="text/javascript"></script>

	<!-- custom css -->
	<link rel="stylesheet" href="">
</head>

<body ng-app="oofme" layout="column">

	<!-- toolbar -->
	<md-toolbar layout="row">
		<h1>oofme</h1>
	</md-toolbar>

	<!-- container -->
	<div flex layout="row">
		<!-- sidebar -->
		<md-sidenav md-is-locked-open="true" class="md-whiteframe-z2"></md-sidenav>
		
		<!-- CONTENT -->
		<md-content flex id="content"></md-content>
	</div>

	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularAnimate %>/angular-animate.min.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularAria %>/angular-aria.min.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularMessages %>/angular-messages.min.js"></script>

	<script src="javascripts/drawer_main.js"></script>

</body>

</html>

<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<title>
		<%= title %>
	</title>


	<!-- Angular Material style sheet -->
	<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/<%= v_angularMaterial %>/angular-material.min.css">

	<!-- google material design icon fonts for web -->
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

	<!-- custom css -->
	<link rel="stylesheet" href="">

	<style>

	</style>
</head>

<body ng-app="oofme" layout="column" ng-clock>

	<!-- toolbar -->
	<md-toolbar layout="row">
		<p>oofme</p>
	</md-toolbar>

	<!-- container -->
	<div flex layout="row">
		<!-- sidebar -->
		<md-sidenav md-is-locked-open="true" class="md-whiteframe-z2">
			<md-list>
				<md-list-item>
					<md-button>
						<md-icon>dashboard</md-icon>
						Home
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>credit_card</md-icon>
						Billing
					</md-button>
				</md-list-item>
	
				<md-divider></md-divider>
				
				<md-list-item>
					<md-button>
						<md-icon>settings</md-icon>
						Settings
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>info</md-icon>
						About
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>help</md-icon>
						Help & Feedback
					</md-button>
				</md-list-item>
			</md-list>
		</md-sidenav>

		<!-- CONTENT -->
		<md-content flex id="content"></md-content>
	</div>

	<!-- Angular JS -->
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularjs %>/angular.min.js" type="text/javascript"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularAnimate %>/angular-animate.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularAria %>/angular-aria.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/<%= v_angularMessages %>/angular-messages.min.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angular_material/<%= v_angularMaterial %>/angular-material.min.js"></script>

	<script src="javascripts/drawer_main.js"></script>

</body>

</html>

<!DOCTYPE html>
<html>

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<title>
		<%= title %>
	</title>

	<!-- Angular Material style sheet -->
	<link rel="stylesheet" href="/node_modules/angular-material/angular-material.min.css">

	<!-- google material design icon fonts for web -->
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

	<!-- roboto fonts -->
	<link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700" rel="stylesheet">

	<!-- custom css -->
	<link rel="stylesheet" href="stylesheets/style.css">

</head>

<body ng-app="oofme" layout="column" ng-cloak>

	<!-- toolbar -->
	<md-toolbar layout="row">
		<p>oofme</p>
	</md-toolbar>

	<!-- container -->
	<div flex layout="row">
		<!-- sidebar -->
		<md-sidenav md-is-locked-open="true" class="md-whiteframe-z2">
			<md-list>

				<md-list-item>
					<md-button>
						<md-icon>dashboard</md-icon>
						Home
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>credit_card</md-icon>
						Billing
					</md-button>
				</md-list-item>

				<md-divider></md-divider>

				<md-list-item>
					<md-button>
						<md-icon>settings</md-icon>
						Settings
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>info</md-icon>
						About
					</md-button>
				</md-list-item>

				<md-list-item>
					<md-button>
						<md-icon>help</md-icon>
						Help & Feedback
					</md-button>
				</md-list-item>
			</md-list>
		</md-sidenav>

		<!-- CONTENT -->
		<md-content flex id="content">
			<div ui-view></div>
		</md-content>
	</div>

	<!-- Angular JS -->
	<script src="/node_modules/angular/angular.min.js" type="text/javascript"></script>
	<script src="/node_modules/angular-ui-router/release/angular-ui-router.min.js" type="text/javascript"></script>
	<script src="/node_modules/angular-animate/angular-animate.min.js"></script>
	<script src="/node_modules/angular-aria/angular-aria.min.js"></script>
	<script src="/node_modules/angular-messages/angular-messages.min.js"></script>
	<script src="/node_modules/angular-material/angular-material.min.js"></script>

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
	<!-- config -->
	<script src="javascripts/drawer_config.js"></script>
	<!-- controllers -->
	<script src="javascripts/controllers/allProjectsCtrl.js"></script>
	<script src="javascripts/controllers/balloonCtrl.js"></script>
	<script src="javascripts/controllers/projectDash_settingsCtrl.js"></script>
	<script src="javascripts/controllers/projectDashCtrl.js"></script>
	<!-- services -->
	<script src="javascripts/services/StoreFactory.js"></script>
</body>

</html>
